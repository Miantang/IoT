#volModel
  .switch.am-fr
    input.cmn-toggle.cmn-toggle-round#cb-12(type="checkbox" value="volume" data-bind="checked:$root.switchValue(), event:{ change: $root.togSwitched}")
    label(for="cb-12")
  ul.inner-boxes.airboxes.am-thumbnails(data-bind="foreach: { data: volButtonGroup, as: 'dv' }")
    li.inner-box.airbox.am-thumbnail.am-btn(data-bind="text:dv.name,attr: {id: 'volbutton-' + dv.id, value : dv.name }, click: $root.controlChanged" data-am-button)
script.
  var vlVM = new volumeViewModel();
  setTimeout(vlVM.loaddata, 200);
  ko.applyBindings(vlVM, document.getElementById("volModel"));
  mainmodel.showback(true);